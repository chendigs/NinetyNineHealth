<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="health.warriors.ninetyninehealth" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MentalHealth"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AnxietyActivity"
            android:label="@string/title_activity_anxietyactivity" />

        <intent-filter>
            <action android:name="health.warriors.ninetyninehealth.AnxietyActivity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
            />
        <activity
            android:name=".DepressionActivity"
            android:label="@string/title_activity_activity_depression" >
        </activity>
        <activity
            android:name="health.warriors.ninetyninehealth.PTSDActivity"
            android:label="PTSD" >
        </activity>
    </application>

</manifest>
